{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/webhook",
      "methods": ["POST"],
      "dest": "index.js"
    },
    {
      "src": "/logs",
      "methods": ["GET"],
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}
